<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tampilanhome">

    <LinearLayout

        android:id="@+id/linearLayout"
        android:layout_width="410dp"
        android:layout_height="69dp"
        android:layout_marginBottom="661dp"
        android:background="#00D1C3"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="15dp"

            app:srcCompat="@drawable/ic_format_list_bulleted_black_24dp" />

        <TextView
            android:id="@+id/muslim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="15dp"
            android:text="Muslim Shop"
            android:textSize="25sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/TV_celana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/lingkaran"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="Celana"
        android:textSize="20dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="87dp" />

    <TextView
        android:id="@+id/TV_Baju"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="176dp"
        android:background="@drawable/lingkaran"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="Baju"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="87dp" />

    <TextView
        android:id="@+id/TV_lain-lain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/lingkaran"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="Lain-Lain"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="297dp"
        tools:layout_editor_absoluteY="87dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/pencarian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#34000000"
        android:ems="10"
        android:hint="pencarian"
        android:inputType="textEmailAddress"
        android:textColor="#DBDBDB"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="128dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="35dp"
        android:layout_height="40dp"
        app:srcCompat="@drawable/ic_search_black_24dp"
        tools:layout_editor_absoluteX="376dp"
        tools:layout_editor_absoluteY="128dp"
        tools:ignore="MissingConstraints"
        android:contentDescription="TODO" />

    <LinearLayout
        android:layout_width="145dp"
        android:layout_height="168dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="460dp">


        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="157dp"
            android:layout_height="match_parent"
            app:srcCompat="@drawable/sajadah" />
    </LinearLayout>

    <ImageView
        android:id="@+id/tasbih"
        android:layout_width="168dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/tasbih"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="195dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="168dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/bajukokoh"
        tools:layout_editor_absoluteX="236dp"
        tools:layout_editor_absoluteY="196dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/TV_tasbih"
        android:layout_width="87dp"
        android:layout_height="36dp"
        android:text="Tasbih"
        android:textSize="25dp"
        android:textStyle="normal|bold|italic"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="49dp"
        tools:layout_editor_absoluteY="357dp" />

    <TextView
        android:id="@+id/TV_harga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RP49.000"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="393dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/TV_bajukokoh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Baju kokoh"
        android:textSize="25dp"
        android:textStyle="normal|bold|italic"
        tools:layout_editor_absoluteX="262dp"
        tools:layout_editor_absoluteY="359dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/TV_hargaB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RP 59.000"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="288dp"
        tools:layout_editor_absoluteY="393dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/TV_sajadah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sajadah"
        android:textSize="25dp"
        android:textStyle="normal|bold|italic"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="636dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/TV_hargaSJDH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rp 115.000"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="678dp"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:layout_width="169dp"
        android:layout_height="165dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="232dp"
        tools:layout_editor_absoluteY="459dp"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/celana1" />
    </LinearLayout>

    <TextView
        android:id="@+id/TV_celana1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Celana Sirval Cream"
        android:textSize="20dp"
        android:textStyle="normal|bold|italic"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="636dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/TV_hargacln"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rp 120.000"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="670dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>